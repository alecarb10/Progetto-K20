<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="txtTM" layoutX="104.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tournament Manager">
         <font>
            <Font size="20.0" />
         </font></Text>
      <TextField fx:id="txtFldName" layoutX="106.0" layoutY="107.0" prefHeight="33.0" prefWidth="188.0" promptText="name" />
      <TextField fx:id="txtFldSurname" layoutX="106.0" layoutY="154.0" prefHeight="33.0" prefWidth="188.0" promptText="surname" />
      <TextField fx:id="txtFldUsername" layoutX="106.0" layoutY="201.0" prefHeight="33.0" prefWidth="188.0" promptText="username" />
      <PasswordField fx:id="pwdFldPassword" layoutX="106.0" layoutY="248.0" prefHeight="33.0" prefWidth="188.0" promptText="password" />
      <PasswordField fx:id="pwdFldRepeatPassword" layoutX="106.0" layoutY="295.0" prefHeight="33.0" prefWidth="188.0" promptText="repeat password" />
      <Button fx:id="btnSignUp" layoutX="106.0" layoutY="347.0" onAction="#registration" prefHeight="17.0" prefWidth="188.0" text="Sign up" />
      <Label fx:id="lblSignIn" layoutX="106.0" layoutY="427.0" onMouseClicked="#signIn" text="Back to sign in" />
   </children>
</AnchorPane>
